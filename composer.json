{
    "name": "codex/filesystems",
    "license": "MIT",
    "type": "codex-addon",
    "description": "A Codex Addon",
    "require": {
        "codex/core": "~2.1",
        "league/flysystem-sftp": "~1.0",
        "league/flysystem-webdav": "~1.0",
        "league/flysystem-ziparchive": "~1.0",
